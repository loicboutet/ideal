<% content_for :title, "Analytique - Administration - Idéal Reprise" %>

<div class="space-y-6">
  <div class="flex items-center justify-between">
    <div>
      <h1 class="text-3xl font-bold text-gray-900">Analytique - Insights</h1>
      <p class="text-gray-600 mt-1">Analyses détaillées pour le développement</p>
    </div>
    
    <!-- Enhanced Export Dropdown -->
    <div class="relative" x-data="{ open: false }">
      <button @click="open = !open" class="px-4 py-2 bg-green-600 text-white rounded-lg hover:bg-green-700 transition flex items-center gap-2">
        <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 10v6m0 0l-3-3m3 3l3-3m2 8H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
        </svg>
        Exporter les données
        <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7"/>
        </svg>
      </button>
      
      <div x-show="open" @click.away="open = false" x-transition class="absolute right-0 mt-2 w-64 bg-white rounded-lg shadow-lg border border-gray-200 py-2 z-10">
        <div class="px-4 py-2 border-b border-gray-200">
          <p class="text-xs font-medium text-gray-700 uppercase">Format d'export</p>
        </div>
        <a href="#" class="flex items-center gap-3 px-4 py-2 hover:bg-gray-50 transition">
          <svg class="w-5 h-5 text-green-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
          </svg>
          <div>
            <p class="text-sm font-medium text-gray-900">Excel (.xlsx)</p>
            <p class="text-xs text-gray-500">Avec graphiques et formatage</p>
          </div>
        </a>
        <a href="#" class="flex items-center gap-3 px-4 py-2 hover:bg-gray-50 transition">
          <svg class="w-5 h-5 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 21h10a2 2 0 002-2V9.414a1 1 0 00-.293-.707l-5.414-5.414A1 1 0 0012.586 3H7a2 2 0 00-2 2v14a2 2 0 002 2z"/>
          </svg>
          <div>
            <p class="text-sm font-medium text-gray-900">CSV (.csv)</p>
            <p class="text-xs text-gray-500">Données brutes</p>
          </div>
        </a>
        <a href="#" class="flex items-center gap-3 px-4 py-2 hover:bg-gray-50 transition">
          <svg class="w-5 h-5 text-red-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 21h10a2 2 0 002-2V9.414a1 1 0 00-.293-.707l-5.414-5.414A1 1 0 0012.586 3H7a2 2 0 00-2 2v14a2 2 0 002 2z"/>
          </svg>
          <div>
            <p class="text-sm font-medium text-gray-900">PDF (.pdf)</p>
            <p class="text-xs text-gray-500">Rapport complet avec graphiques</p>
          </div>
        </a>
        <div class="border-t border-gray-200 mt-2 pt-2">
          <div class="px-4 py-2">
            <p class="text-xs font-medium text-gray-700 uppercase mb-2">Portée de l'export</p>
            <div class="space-y-1">
              <label class="flex items-center gap-2 text-sm text-gray-700">
                <input type="radio" name="export_scope" value="current" checked class="text-admin-600 focus:ring-admin-500">
                Vue actuelle
              </label>
              <label class="flex items-center gap-2 text-sm text-gray-700">
                <input type="radio" name="export_scope" value="filtered" class="text-admin-600 focus:ring-admin-500">
                Données filtrées
              </label>
              <label class="flex items-center gap-2 text-sm text-gray-700">
                <input type="radio" name="export_scope" value="all" class="text-admin-600 focus:ring-admin-500">
                Toutes les données
              </label>
            </div>
          </div>
        </div>
        <div class="border-t border-gray-200 mt-2 pt-2 px-4 py-2">
          <button class="w-full px-4 py-2 bg-admin-600 text-white rounded-lg hover:bg-admin-700 transition text-sm font-medium">
            Confirmer l'export
          </button>
        </div>
      </div>
    </div>
  </div>

  <!-- Period Selector -->
  <div class="bg-white rounded-xl shadow-sm p-4 border border-gray-200">
    <div class="grid grid-cols-1 sm:grid-cols-4 gap-4">
      <select class="px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-admin-500">
        <option>30 derniers jours</option>
        <option>3 mois</option>
        <option>6 mois</option>
        <option>1 an</option>
        <option>Personnalisé...</option>
      </select>
      <select class="px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-admin-500">
        <option>Tous les secteurs</option>
        <option>Industrie</option>
        <option>BTP</option>
        <option>Commerce & Distribution</option>
        <option>Services</option>
      </select>
      <select class="px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-admin-500">
        <option>Toutes les régions</option>
        <option>Île-de-France</option>
        <option>Auvergne-Rhône-Alpes</option>
        <option>Nouvelle-Aquitaine</option>
      </select>
      <button class="px-4 py-2 bg-admin-600 text-white rounded-lg hover:bg-admin-700 transition font-medium">
        Appliquer les filtres
      </button>
    </div>
  </div>

  <!-- Time Series Charts -->
  <div class="bg-white rounded-xl shadow-sm border border-gray-200">
    <div class="p-6 border-b border-gray-200">
      <h2 class="text-lg font-bold text-gray-900">Évolution temporelle</h2>
      <p class="text-sm text-gray-600 mt-1">Suivi des métriques clés dans le temps</p>
    </div>
    
    <div class="grid grid-cols-1 lg:grid-cols-2 gap-6 p-6">
      <!-- Chart 1: Annonces actives -->
      <div class="border border-gray-200 rounded-lg p-4">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-sm font-semibold text-gray-900">Annonces actives</h3>
          <span class="text-xs px-2 py-1 bg-green-100 text-green-800 rounded-full">+12%</span>
        </div>
        <div class="h-48 flex items-end justify-between gap-1">
          <% [245, 258, 267, 283, 295, 302, 315, 327].each_with_index do |value, idx| %>
            <div class="flex-1 flex flex-col items-center group cursor-pointer">
              <div class="w-full bg-admin-400 hover:bg-admin-500 rounded-t transition-colors" style="height: <%= (value.to_f / 327 * 160).to_i %>px;"></div>
              <span class="text-xs text-gray-500 mt-2"><%= idx == 0 ? 'J-7' : idx == 7 ? 'Auj' : '' %></span>
            </div>
          <% end %>
        </div>
      </div>

      <!-- Chart 2: Réservations -->
      <div class="border border-gray-200 rounded-lg p-4">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-sm font-semibold text-gray-900">Réservations</h3>
          <span class="text-xs px-2 py-1 bg-green-100 text-green-800 rounded-full">+18%</span>
        </div>
        <div class="h-48 flex items-end justify-between gap-1">
          <% [98, 105, 112, 120, 128, 135, 138, 142].each_with_index do |value, idx| %>
            <div class="flex-1 flex flex-col items-center group cursor-pointer">
              <div class="w-full bg-green-400 hover:bg-green-500 rounded-t transition-colors" style="height: <%= (value.to_f / 142 * 160).to_i %>px;"></div>
              <span class="text-xs text-gray-500 mt-2"><%= idx == 0 ? 'J-7' : idx == 7 ? 'Auj' : '' %></span>
            </div>
          <% end %>
        </div>
      </div>

      <!-- Chart 3: Transactions -->
      <div class="border border-gray-200 rounded-lg p-4">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-sm font-semibold text-gray-900">Transactions complétées</h3>
          <span class="text-xs px-2 py-1 bg-green-100 text-green-800 rounded-full">+9%</span>
        </div>
        <div class="h-48 flex items-end justify-between gap-1">
          <% [32, 35, 38, 42, 45, 48, 52, 54].each_with_index do |value, idx| %>
            <div class="flex-1 flex flex-col items-center group cursor-pointer">
              <div class="w-full bg-blue-400 hover:bg-blue-500 rounded-t transition-colors" style="height: <%= (value.to_f / 54 * 160).to_i %>px;"></div>
              <span class="text-xs text-gray-500 mt-2"><%= idx == 0 ? 'J-7' : idx == 7 ? 'Auj' : '' %></span>
            </div>
          <% end %>
        </div>
      </div>

      <!-- Chart 4: Revenus -->
      <div class="border border-gray-200 rounded-lg p-4">
        <div class="flex items-center justify-between mb-4">
          <h3 class="text-sm font-semibold text-gray-900">Revenus (€)</h3>
          <span class="text-xs px-2 py-1 bg-green-100 text-green-800 rounded-full">+24%</span>
        </div>
        <div class="h-48 flex items-end justify-between gap-1">
          <% [32500, 35200, 38400, 41200, 43800, 45600, 47200, 48562].each_with_index do |value, idx| %>
            <div class="flex-1 flex flex-col items-center group cursor-pointer">
              <div class="w-full bg-yellow-400 hover:bg-yellow-500 rounded-t transition-colors" style="height: <%= (value.to_f / 48562 * 160).to_i %>px;"></div>
              <span class="text-xs text-gray-500 mt-2"><%= idx == 0 ? 'J-7' : idx == 7 ? 'Auj' : '' %></span>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>

  <!-- Temps moyen par statut CRM -->
  <div class="bg-white rounded-xl shadow-sm p-6 border border-gray-200">
    <h2 class="text-lg font-bold text-gray-900 mb-6">Temps moyen par statut CRM</h2>
    <p class="text-sm text-gray-600 mb-4">Avec marquage du temps maximum permis</p>
    
    <div class="space-y-3">
      <% [
        {name: 'À contacter', avg: 4.2, max: 7, unit: 'jours'},
        {name: 'Échange d\'infos', avg: 18.5, max: 33, unit: 'jours'},
        {name: 'Analyse', avg: 8.3, max: 33, unit: 'jours'},
        {name: 'Alignement projets', avg: 6.8, max: 33, unit: 'jours'},
        {name: 'Négociation', avg: 12.4, max: 20, unit: 'jours'},
        {name: 'LOI', avg: 15.2, max: nil, unit: 'jours'},
        {name: 'Audits', avg: 21.3, max: nil, unit: 'jours'},
        {name: 'Financement', avg: 18.7, max: nil, unit: 'jours'}
      ].each do |stat| %>
        <div>
          <div class="flex items-center justify-between mb-2">
            <span class="text-sm font-medium text-gray-700"><%= stat[:name] %></span>
            <div class="text-sm text-gray-900">
              Moyenne: <strong><%= stat[:avg] %> <%= stat[:unit] %></strong>
              <% if stat[:max] %>
                / Max: <strong class="text-red-600"><%= stat[:max] %>j</strong>
              <% end %>
            </div>
          </div>
          <div class="relative">
            <div class="w-full bg-gray-200 rounded-full h-3">
              <% if stat[:max] %>
                <div class="bg-<%= stat[:avg] > stat[:max] * 0.8 ? 'red' : 'green' %>-500 h-3 rounded-full" style="width: <%= [stat[:avg] / stat[:max] * 100, 100].min %>%"></div>
                <div class="absolute top-0 h-3 w-0.5 bg-red-600" style="left: 100%"></div>
              <% else %>
                <div class="bg-admin-500 h-3 rounded-full" style="width: 60%"></div>
              <% end %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>

  <!-- Interactive Pivot Table -->
  <div class="bg-white rounded-xl shadow-sm border border-gray-200">
    <div class="p-6 border-b border-gray-200">
      <div class="flex flex-col lg:flex-row lg:items-center lg:justify-between gap-4">
        <div>
          <h2 class="text-lg font-bold text-gray-900">Tableau croisé dynamique</h2>
          <p class="text-sm text-gray-600 mt-1">Analysez les données selon plusieurs dimensions</p>
        </div>
        <div class="flex flex-wrap gap-3">
          <select class="px-3 py-2 text-sm border border-gray-300 rounded-lg focus:ring-2 focus:ring-admin-500 bg-white">
            <option>Dimension lignes: Secteur</option>
            <option>Dimension lignes: CA</option>
            <option>Dimension lignes: Géographie</option>
            <option>Dimension lignes: Effectif</option>
            <option>Dimension lignes: Type de deal</option>
          </select>
          <select class="px-3 py-2 text-sm border border-gray-300 rounded-lg focus:ring-2 focus:ring-admin-500 bg-white">
            <option>Dimension colonnes: Période</option>
            <option>Dimension colonnes: Secteur</option>
            <option>Dimension colonnes: Région</option>
            <option>Dimension colonnes: Type repreneur</option>
          </select>
          <select class="px-3 py-2 text-sm border border-gray-300 rounded-lg focus:ring-2 focus:ring-admin-500 bg-white">
            <option>Métrique: Annonces</option>
            <option>Métrique: Réservations</option>
            <option>Métrique: Transactions</option>
            <option>Métrique: Revenus</option>
            <option>Métrique: Taux conversion</option>
          </select>
        </div>
      </div>
    </div>

    <div class="p-6">
      <div class="overflow-x-auto">
        <table class="min-w-full divide-y divide-gray-200">
          <thead class="bg-gray-50">
            <tr>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase sticky left-0 bg-gray-50">
                Secteur
              </th>
              <th class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase">
                <div>Sem 1</div>
                <div class="text-xs font-normal text-gray-400">Jan 1-7</div>
              </th>
              <th class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase">
                <div>Sem 2</div>
                <div class="text-xs font-normal text-gray-400">Jan 8-14</div>
              </th>
              <th class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase">
                <div>Sem 3</div>
                <div class="text-xs font-normal text-gray-400">Jan 15-21</div>
              </th>
              <th class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase">
                <div>Sem 4</div>
                <div class="text-xs font-normal text-gray-400">Jan 22-28</div>
              </th>
              <th class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase bg-admin-50">
                Total
              </th>
              <th class="px-4 py-3 text-center text-xs font-medium text-gray-500 uppercase bg-green-50">
                Évolution
              </th>
            </tr>
          </thead>
          <tbody class="bg-white divide-y divide-gray-200">
            <% [
              {sector: 'Commerce & Distribution', w1: 28, w2: 32, w3: 35, w4: 32, evolution: '+14%'},
              {sector: 'Hôtellerie / Restauration', w1: 22, w2: 25, w3: 27, w4: 24, evolution: '+9%'},
              {sector: 'Services', w1: 18, w2: 21, w3: 23, w4: 22, evolution: '+22%'},
              {sector: 'Digital', w1: 15, w2: 18, w3: 20, w4: 19, evolution: '+27%'},
              {sector: 'BTP', w1: 14, w2: 16, w3: 18, w4: 17, evolution: '+21%'},
              {sector: 'Industrie', w1: 11, w2: 13, w3: 15, w4: 13, evolution: '+18%'},
              {sector: 'Transport & logistique', w1: 10, w2: 12, w3: 13, w4: 13, evolution: '+30%'},
              {sector: 'Santé', w1: 8, w2: 10, w3: 11, w4: 9, evolution: '+13%'},
              {sector: 'Agroalimentaire', w1: 7, w2: 8, w3: 9, w4: 8, evolution: '+14%'},
              {sector: 'Immobilier', w1: 6, w2: 7, w3: 8, w4: 7, evolution: '+17%'},
              {sector: 'Autre', w1: 5, w2: 6, w3: 6, w4: 7, evolution: '+40%'}
            ].each do |row| %>
              <tr class="hover:bg-gray-50 cursor-pointer" onclick="alert('Détails pour <%= row[:sector] %>')">
                <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900 sticky left-0 bg-white">
                  <div class="flex items-center gap-2">
                    <svg class="w-4 h-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                    </svg>
                    <%= row[:sector] %>
                  </div>
                </td>
                <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900"><%= row[:w1] %></td>
                <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900"><%= row[:w2] %></td>
                <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900"><%= row[:w3] %></td>
                <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900"><%= row[:w4] %></td>
                <td class="px-4 py-4 whitespace-nowrap text-sm text-center font-semibold text-gray-900 bg-admin-50">
                  <%= row[:w1] + row[:w2] + row[:w3] + row[:w4] %>
                </td>
                <td class="px-4 py-4 whitespace-nowrap text-sm text-center font-medium bg-green-50 <%= row[:evolution].starts_with?('+') ? 'text-green-600' : 'text-red-600' %>">
                  <%= row[:evolution] %>
                </td>
              </tr>
            <% end %>
            <tr class="bg-gray-50 font-semibold">
              <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-900 sticky left-0 bg-gray-50">
                Total
              </td>
              <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900">144</td>
              <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900">168</td>
              <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900">185</td>
              <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900">171</td>
              <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-gray-900 bg-admin-100">668</td>
              <td class="px-4 py-4 whitespace-nowrap text-sm text-center text-green-600 bg-green-100">+19%</td>
            </tr>
          </tbody>
        </table>
      </div>
      
      <div class="mt-4 flex items-center justify-between text-sm text-gray-500">
        <div class="flex items-center gap-2">
          <svg class="w-4 h-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
          </svg>
          Cliquez sur une ligne pour voir les détails
        </div>
        <div class="flex items-center gap-3">
          <button class="text-admin-600 hover:text-admin-700 font-medium">Exporter ce tableau</button>
          <button class="text-admin-600 hover:text-admin-700 font-medium">Réinitialiser les filtres</button>
        </div>
      </div>
    </div>
  </div>

  <!-- Détails multi-axes (existing detailed analysis) -->
  <div class="bg-white rounded-xl shadow-sm border border-gray-200">
    <div class="p-6 border-b border-gray-200">
      <h2 class="text-lg font-bold text-gray-900">Analyses détaillées par dimension</h2>
      <p class="text-sm text-gray-600 mt-1">Vue standard des métriques principales</p>
    </div>
    
    <!-- Tabs -->
    <div class="border-b border-gray-200" x-data="{ activeTab: 'sector' }">
      <nav class="flex px-6 overflow-x-auto">
        <button @click="activeTab = 'sector'" :class="activeTab === 'sector' ? 'border-admin-500 text-admin-600' : 'border-transparent text-gray-500 hover:text-gray-700'" class="px-4 py-3 border-b-2 font-medium text-sm whitespace-nowrap">
          Par Secteur
        </button>
        <button @click="activeTab = 'revenue'" :class="activeTab === 'revenue' ? 'border-admin-500 text-admin-600' : 'border-transparent text-gray-500 hover:text-gray-700'" class="px-4 py-3 border-b-2 font-medium text-sm whitespace-nowrap">
          Par CA
        </button>
        <button @click="activeTab = 'geography'" :class="activeTab === 'geography' ? 'border-admin-500 text-admin-600' : 'border-transparent text-gray-500 hover:text-gray-700'" class="px-4 py-3 border-b-2 font-medium text-sm whitespace-nowrap">
          Par Géographie
        </button>
        <button @click="activeTab = 'employees'" :class="activeTab === 'employees' ? 'border-admin-500 text-admin-600' : 'border-transparent text-gray-500 hover:text-gray-700'" class="px-4 py-3 border-b-2 font-medium text-sm whitespace-nowrap">
          Par Effectif
        </button>
      </nav>
    </div>

    <div class="p-6">
      <div class="overflow-x-auto">
        <table class="min-w-full divide-y divide-gray-200">
          <thead class="bg-gray-50">
            <tr>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase">Secteur</th>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase">Annonces</th>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase">Réservations</th>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase">Transactions</th>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase">Taux conversion</th>
              <th class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase">Évolution</th>
            </tr>
          </thead>
          <tbody class="bg-white divide-y divide-gray-200">
            <% [
              {sector: 'Commerce & Distribution', listings: 127, reservations: 89, transactions: 45, conversion: '35.4%', evolution: '+12%'},
              {sector: 'Hôtellerie / Restauration', listings: 98, reservations: 72, transactions: 38, conversion: '38.8%', evolution: '+8%'},
              {sector: 'Services', listings: 84, reservations: 65, transactions: 32, conversion: '38.1%', evolution: '+15%'},
              {sector: 'Digital', listings: 72, reservations: 58, transactions: 28, conversion: '38.9%', evolution: '+22%'},
              {sector: 'BTP', listings: 65, reservations: 42, transactions: 24, conversion: '36.9%', evolution: '+5%'},
              {sector: 'Industrie', listings: 52, reservations: 35, transactions: 18, conversion: '34.6%', evolution: '+3%'},
              {sector: 'Transport & logistique', listings: 48, reservations: 32, transactions: 15, conversion: '31.3%', evolution: '+7%'},
              {sector: 'Santé', listings: 38, reservations: 28, transactions: 12, conversion: '31.6%', evolution: '+18%'},
              {sector: 'Agroalimentaire & Agriculture', listings: 32, reservations: 22, transactions: 10, conversion: '31.3%', evolution: '+6%'},
              {sector: 'Immobilier', listings: 28, reservations: 18, transactions: 8, conversion: '28.6%', evolution: '+4%'},
